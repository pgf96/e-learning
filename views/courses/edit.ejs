<%- include('../partials/header', {active: "" }) %>

<div>Edit your title</div>

<div class="creating-course-container">
  <form action="/courses/<%= course.id %>?_method=PUT" method="post">
    <div class="form-group">
      <label for="course-title">Title</label>
      <input
        type="text"
        class="form-control"
        id="course-title"
        aria-describedby="emailHelp"
        placeholder="Tune Up Your Music Skills "
        name="title"
        value="<%= course.title %>"
      />
    </div>
    <button type="submit" class="btn btn-primary bg-secondary" disabled>
      Add Learning Objective
    </button>
    <button type="submit" class="btn btn-primary" id="submit">Continue</button>
  </form>
</div>

<%- include('../partials/footer') %>
